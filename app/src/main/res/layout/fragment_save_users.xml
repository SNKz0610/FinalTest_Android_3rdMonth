<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data></data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#000000"
        tools:context=".fragment.SaveUsersFragment">

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/rev_fragsaveusers"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_margin="10dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toTopOf="@id/button_fragsaveuser_back"/>
        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/button_fragsaveuser_show"
            android:layout_width="wrap_content"
            android:layout_height="50dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintRight_toLeftOf="@id/button_fragsaveuser_back"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginBottom="20dp"
            android:background="#60B7FB"
            android:text="Show"
            android:padding="10dp"/>

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/button_fragsaveuser_back"
            android:layout_width="wrap_content"
            android:layout_height="50dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toRightOf="@id/button_fragsaveuser_show"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginBottom="20dp"
            android:background="#60B7FB"
            android:text="Back"
            android:padding="10dp"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>